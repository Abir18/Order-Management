<mvc:View
	controllerName="task.order.management.controller.App"
	displayBlock="true"
	height="100%"
	xmlns="sap.f"
	xmlns:mvc="sap.ui.core.mvc"
>
	<FlexibleColumnLayout
		id="flexibleColumnLayout"
		backgroundDesign="Solid"
	>
		<beginColumnPages>
			<mvc:XMLView
				id="beginView"
				viewName="task.order.management.view.Master"
			/>
		</beginColumnPages>
		<midColumnPages>
			<mvc:XMLView
				id="detailView"
				viewName="task.order.management.view.Detail"
			/>
		</midColumnPages>
	</FlexibleColumnLayout>
</mvc:View>