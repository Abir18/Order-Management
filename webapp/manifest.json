{
    "_version": "1.12.0",
    "sap.app": {
        "id": "task.order.management",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "task.order.management.view.App",
            "type": "XML",
            "async": true,
            "id": "fcl"
        },
        "dependencies": {
            "minUI5Version": "1.98.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {},
                "sap.uxap": {}
            }
        },
        "config": {
            "fullWidth": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "task.order.management.i18n.i18n"
                },
                "supportedLocales": ["", "en"],
                "fallbackLocale": ""
            },
            "countries": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "data/countries.json"
            }
        }
    }
}
